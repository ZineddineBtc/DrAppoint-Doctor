        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.TermsActivity"></activity>
        <activity android:name=".activities.entry.SetProfileActivity" />
        <activity android:name=".activities.entry.RegisterActivity" />
        <activity
            android:name=".activities.core.CoreActivity"
            android:label="@string/title_activity_core" />
        <activity android:name=".activities.entry.LoginActivity" />
        <activity android:name=".activities.entry.SliderActivity" />
        <activity android:name=".activities.entry.LogoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>